<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<%_ for (const entry of entries) { _%>
	<url>
		<loc>https://w0s.jp<%= entry.pagePathAbsoluteUrl %></loc>
		<%_ if (entry.modified_at !== undefined) { _%>
		<lastmod><%= entry.modified_at.format('YYYY-MM-DD') %></lastmod>
		<%_ } _%>
	</url>
	<%_ } _%>
	<url>
		<loc>https://w0s.jp/tokyu/data/history/</loc>
	</url>
	<url>
		<loc>https://w0s.jp/kumeta/akamatsu-generator</loc>
	</url>
</urlset>
