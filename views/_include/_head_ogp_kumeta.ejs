<meta property="og:title" content="<%= page.title %>" />
<meta property="og:type" content="<% if (page.path === '/kumeta/') { %>website<% } else { %>article<% } %>" />
<meta property="og:image" content="https://w0s.jp/assets/image/site-icon.png" />
<meta property="og:image:width" content="360" />
<meta property="og:image:height" content="360" />
<meta property="og:url" content="https://w0s.jp<%= page.path %>" />
<meta property="og:locale" content="ja_JP" />
<meta property="og:site_name" content="久米田康治データベース" />
<%_ if (page.description !== '') { _%>
<meta property="og:description" content="<%= page.description %>" />
<meta name="twitter:card" content="summary" />
<meta name="twitter:site" content="@SaekiTominaga" />
<%_ } _%>
