{
	"name": "@w0s.jp/astro",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "astro dev",
		"build-dev": "astro check && tsc --noEmit && run-s _astro-build:*",
		"build-prod": "NODE_OPTIONS=\"--max-old-space-size=500\" run-s _astro-build:*",
		"lint": "eslint src/**/*.{astro,ts}",
		"node-lint": "eslint build/*.js",
		"html-lint": "markuplint dist/client/**/*.html",
		"css-build-watch": "postcss style/*.css --base style -d public/assets/style -w",
		"css-build-all": "postcss style/*.css --base style -d public/assets/style",
		"css-lint": "stylelint style/**/*.css",
		"js-build-watch": "rollup -c -w",
		"js-build-all": "rollup -c",
		"js-lint": "eslint script/**/*.ts",
		"_astro-build:build": "astro build",
		"_astro-build:html": "node build/html.js -d dist/client",
		"_astro-build:svg": "node build/svg.js -f dist/client/**/*.svg -f dist/client/favicon.ico -c svgo.config.cjs",
		"_astro-build:feed": "node build/feed.js -d dist/client",
		"_astro-build:sitemap": "node build/sitemap.js -d dist/client --ignore 4** --ignore 5** --ignore admin -t template/xml/sitemap.ejs -o sitemap.xml",
		"_astro-build:format": "prettier --config ../.prettierrc.mjs -w dist/client/**/*.{html,css}",
		"_astro-build:compress": "brotli-cli compress -g dist/client/**/*.{html,css,js,mjs,svg,atom} -g dist/client/favicon.ico -m text"
	},
	"dependencies": {
		"@astrojs/node": "^5.3.1",
		"@log4js-node/smtp": "^2.0.8",
		"@saekitominaga/closest-html-page": "^2.0.1",
		"@saekitominaga/customelements-button-checkboxes-ctrl": "^1.2.0",
		"@saekitominaga/customelements-button-clipboard": "^1.1.4",
		"@saekitominaga/customelements-button-confirm": "^1.1.4",
		"@saekitominaga/customelements-button-media-sameplay": "^1.1.4",
		"@saekitominaga/customelements-details-animation": "^3.0.0",
		"@saekitominaga/customelements-input-date-totext": "^1.1.4",
		"@saekitominaga/customelements-input-switch": "^2.4.1",
		"@saekitominaga/customelements-portal": "^2.4.1",
		"@saekitominaga/customelements-tab": "^1.6.6",
		"@saekitominaga/customelements-textarea-autoheight": "^1.2.2",
		"@saekitominaga/customelements-tooltip-trigger": "^2.1.2",
		"@saekitominaga/htmlformcontrolelement-validation": "^2.0.2",
		"@saekitominaga/htmlformelement-before-unload-confirm": "^1.2.4",
		"@saekitominaga/htmlformelement-submit-overlay": "^2.1.0",
		"@saekitominaga/htmltablecellelement-ditto": "^2.0.0",
		"@saekitominaga/isbn-verify": "^2.0.1",
		"@saekitominaga/paapi-item-image-url-parser": "^2.2.4",
		"@saekitominaga/report-js-error": "^1.2.3",
		"@saekitominaga/report-same-referrer": "^1.2.1",
		"@saekitominaga/string-convert": "^1.0.4",
		"@saekitominaga/string-escape-html": "^2.0.1",
		"@saekitominaga/urlsearchparams-custom-separator": "^2.0.0",
		"astro": "^2.9.7",
		"dayjs": "^1.11.9",
		"diff": "^5.1.0",
		"ejs": "^3.1.9",
		"github-slugger": "^2.0.0",
		"glob": "^10.3.3",
		"highlight.js": "^11.8.0",
		"jsdom": "^22.1.0",
		"log4js": "^6.9.1",
		"md5": "^2.3.0",
		"nodemailer": "^6.9.4",
		"prettier": "^3.0.0",
		"slash": "^5.1.0",
		"sqlite": "^5.0.1",
		"sqlite3": "^5.1.6",
		"svgo": "^3.0.2",
		"xml-formatter": "^3.4.1"
	},
	"devDependencies": {
		"@types/diff": "^5.0.3",
		"@types/ejs": "^3.1.2",
		"@types/express": "^4.17.17",
		"@types/jsdom": "^21.1.1",
		"@types/md5": "^2.3.2",
		"@types/nodemailer": "^6.4.9",
		"aws-sdk": "^2.1428.0",
		"mock-aws-s3": "^4.0.2",
		"nock": "^13.3.2"
	}
}
