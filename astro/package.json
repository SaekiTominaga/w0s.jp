{
	"name": "@w0s.jp/astro",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "astro dev",
		"build-dev": "astro check && tsc --noEmit && run-s _astro-build:*",
		"build-prod": "NODE_OPTIONS=\"--max-old-space-size=500\" run-s _astro-build:*",
		"lint": "eslint src/**/*.{astro,ts}",
		"node-lint": "eslint build/*.js",
		"html-lint": "markuplint dist/client/**/*.html",
		"css-build-watch": "postcss style/*.css --base style -d public/assets/style -w",
		"css-build-all": "postcss style/*.css --base style -d public/assets/style",
		"css-lint": "stylelint style/**/*.css",
		"js-build-watch": "rollup -c -w",
		"js-build-all": "rollup -c",
		"js-lint": "eslint script/**/*.ts",
		"_astro-build:build": "astro build",
		"_astro-build:html": "node build/html.js -d dist/client",
		"_astro-build:svg": "node build/svg.js -f dist/client/**/*.svg -f dist/client/favicon.ico -c svgo.config.cjs",
		"_astro-build:feed": "node build/feed.js -d dist/client",
		"_astro-build:sitemap": "node build/sitemap.js -d dist/client --ignore 4** --ignore 5** --ignore admin -t template/xml/sitemap.ejs -o sitemap.xml",
		"_astro-build:format": "prettier --config ../.prettierrc.mjs -w dist/client/**/*.{html,css}",
		"_astro-build:compress": "brotli-cli compress -g dist/client/**/*.{html,css,js,mjs,svg,atom} -g dist/client/favicon.ico -m text"
	},
	"dependencies": {
		"@astrojs/check": "^0.4.1",
		"@astrojs/node": "^5.3.6",
		"@log4js-node/smtp": "^2.0.8",
		"@types/trusted-types": "^2.0.7",
		"@w0s/button-checkboxes": "^2.0.0",
		"@w0s/button-clipboard": "^2.0.0",
		"@w0s/button-confirm": "^2.0.0",
		"@w0s/button-media-same-play": "^2.0.0",
		"@w0s/closest-html-page": "^3.0.0",
		"@w0s/details-animation": "^4.0.0",
		"@w0s/footnote-reference-popover": "^4.1.0",
		"@w0s/form-control-validation": "^3.0.0",
		"@w0s/form-submit-overlay": "^3.0.0",
		"@w0s/html-escape": "^3.0.0",
		"@w0s/input-date-to-text": "^2.0.0",
		"@w0s/input-switch": "^4.0.0",
		"@w0s/isbn-verify": "^3.0.0",
		"@w0s/paapi-item-image-url-parser": "^3.0.0",
		"@w0s/portal-animation": "^3.0.1",
		"@w0s/report-js-error": "^2.0.0",
		"@w0s/report-same-referrer": "^3.0.0",
		"@w0s/string-convert": "^2.0.0",
		"@w0s/table-cell-ditto": "^4.0.0",
		"@w0s/textarea-auto-size": "^2.0.0",
		"@w0s/urlsearchparams-custom-separator": "^3.0.0",
		"astro": "^2.10.15",
		"dayjs": "^1.11.9",
		"diff": "^5.1.0",
		"ejs": "^3.1.9",
		"github-slugger": "^2.0.0",
		"glob": "^10.3.3",
		"highlight.js": "^11.8.0",
		"jsdom": "^22.1.0",
		"log4js": "^6.9.1",
		"md5": "^2.3.0",
		"nodemailer": "^6.9.9",
		"prettier": "^3.0.0",
		"slash": "^5.1.0",
		"sqlite": "^5.0.1",
		"sqlite3": "^5.1.6",
		"svgo": "^3.0.2",
		"xml-formatter": "^3.4.1"
	},
	"devDependencies": {
		"@types/diff": "^5.0.3",
		"@types/ejs": "^3.1.2",
		"@types/express": "^4.17.17",
		"@types/jsdom": "^21.1.1",
		"@types/md5": "^2.3.2",
		"@types/nodemailer": "^6.4.9",
		"aws-sdk": "^2.1428.0",
		"mock-aws-s3": "^4.0.2",
		"nock": "^13.3.2"
	}
}
