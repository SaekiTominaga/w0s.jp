---
import dayjs from 'dayjs';
import Layout from '@layouts/Tokyu.astro';
import type { StructuredData } from '../../../types/types.js';

const structuredData: StructuredData = {
	title: '設定器配列　東急田園都市線　8500系（自動放送搭載編成、2005年〜2006年）',
	heading: '田園都市線　8500系',
	subHeading: '自動放送搭載編成、2005年〜2006年',
	dateModified: dayjs('2006-01-02'),
	breadcrumb: [
		{ path: '/tokyu/', name: 'ホーム' },
		{ path: '/tokyu/set/', name: '設定器配列' },
	],
	localNav: [
		{ path: '/tokyu/set/8500_denentoshi_announce_2005', name: '2005年〜2006年' },
		{ path: '/tokyu/set/8500_denentoshi_announce_2006', name: '2006年〜' },
		{ path: '/tokyu/set/8500_denentoshi_announce_2022', name: '2022年〜' },
	],
};
---

<Layout astroFilePath={Astro.self.moduleId} structuredData={structuredData}>
	<div class="l-tokyu-set">
		<div class="l-tokyu-set__header">
			<p class="p-tokyu-set-period">2005年<!-- 12月 -->より始まった自動放送取付けから、2006年<!-- 3月 -->の乗入れ区間久喜延長対応まで</p>
			<p class="p-tokyu-set-note">自動放送取付け編成の下り方先頭車に設置</p>
		</div>
		<div class="l-tokyu-set__main">
			<div class="p-tokyu-set-panel -push">
				<object data="/tokyu/set/image/8500_denentoshi_announce_2005.svg" type="image/svg+xml" width="720" height="740" class="p-tokyu-set-panel__image" role="img" aria-labelledby="set-image-alt1">
					<div id="set-image-alt1">
						<section>
							<h2>始発駅</h2>
							<table>
								<tbody>
									<tr>
										<td>中央林間</td>
										<td>長津田</td>
