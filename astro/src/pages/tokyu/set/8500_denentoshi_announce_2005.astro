									</tr>
									<tr>
										<td></td>
										<td>急行南停</td>
									</tr>
								</tbody>
							</table>
						</section>

						<section>
							<h2>行先</h2>
							<table>
								<tbody>
									<tr>
										<td></td>
										<td></td>
										<td></td>
										<td>中央林間</td>
										<td></td>
										<td>長津田</td>
