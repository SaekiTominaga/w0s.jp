---
import dayjs from 'dayjs';
import Layout from '@layouts/Tokyu.astro';
import ListNote from '@components/ListNote.astro';
import type { StructuredData } from '@type/types.js';

const structuredData: StructuredData = {
	title: '設定器配列　東急田園都市線　8500系（東武乗入れ編成、2006年〜）',
	heading: '田園都市線　8500系',
	subHeading: '東武乗入れ編成、2006年〜',
	dateModified: dayjs('2013-04-14'),
	breadcrumb: [
		{ path: '/tokyu/', name: '東急電車資料室' },
		{ path: '/tokyu/set/', name: '設定器配列' },
	],
	localNav: {
		label: '田園都市線8500系（東武乗入れ編成）の設定器バリエーション',
		items: [
			{ path: '/tokyu/set/8500_denentoshi_tobu_2003', name: '2003年〜2006年' },
			{ path: '/tokyu/set/8500_denentoshi_tobu_2006', name: '2006年〜' },
		],
	},
};
---

<Layout astroFilePath={Astro.self.moduleId} structuredData={structuredData} toc={false}>
	<div class="l-tokyu-set">
		<div class="l-tokyu-set__header">
			<p class="p-tokyu-set-period">2006年<!-- 3月 -->の乗入れ久喜延長対応から</p>
			<p class="p-tokyu-set-note">自動放送取付け編成を除く、東武乗入れ編成の下り方先頭車に設置</p>
		</div>
		<div class="l-tokyu-set__main">
			<div class="p-tokyu-set-panel -push">
				<object data="/tokyu/set/image/8500_denentoshi_tobu_2006.svg" type="image/svg+xml" width="720" height="740" class="p-tokyu-set-panel__image" role="img" aria-labelledby="set-image-alt1">
					<div id="set-image-alt1">
						<section>
							<h2>始発駅</h2>
							<table>
								<tbody>
									<tr>
										<td></td>
