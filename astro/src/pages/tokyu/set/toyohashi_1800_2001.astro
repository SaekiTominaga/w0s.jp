---
import dayjs from 'dayjs';
import Layout from '@layouts/Tokyu.astro';
import type { StructuredData } from '../../../types/types.js';

const structuredData: StructuredData = {
	title: '設定器配列　豊橋鉄道　新1800系',
	heading: '豊橋鉄道　新1800系',
	dateModified: dayjs('2006-02-25'),
	breadcrumb: [
		{ path: '/tokyu/', name: 'ホーム' },
		{ path: '/tokyu/set/', name: '設定器配列' },
	],
};
---

<Layout astroFilePath={Astro.self.moduleId} structuredData={structuredData}>
	<div class="l-tokyu-set">
		<div class="l-tokyu-set__header">
			<p class="p-tokyu-set-note">電動字幕を装備する先頭車に設置</p>
		</div>
		<div class="l-tokyu-set__main">
			<div class="p-tokyu-set-panel -dial">
				<object data="/tokyu/set/image/toyohashi_1800_2001.png" type="image/png" width="640" height="640" class="p-tokyu-set-panel__image" role="img" aria-labelledby="set-image-alt1">
					<ul id="set-image-alt1">
						<li>1: 奥沢</li>
						<li>2: 田園調布</li>
						<li>3: 目黒</li>
						<li>4: 蒲田</li>
						<li>5: 五反田</li>
						<li>6: 雪が谷大塚</li>
						<li>7: 大井町</li>
						<li>8: 二子玉川園</li>
						<li>9: 自由が丘</li>
						<li>10: 梶が谷</li>
						<li>11: 鷺沼</li>
						<li>12: 長津田</li>
						<li>13: こどもの国</li>
						<li>14: (空欄)</li>
						<li>15: 回送</li>
						<li>16: 試運転</li>
						<li>17: 臨時</li>
						<li>18: 団体</li>
						<li>19: 新豊橋</li>
						<li>20: 三河田原</li>
						<li>21: 高師</li>
						<li>22: 大清水</li>
						<li>23: 老津</li>
						<li>24: (空欄)</li>
						<li>25: (空欄)</li>
						<li>26: (空欄)</li>
						<li>27: (空欄)</li>
						<li>28: (空欄)</li>
						<li>29: (空欄)</li>
						<li>30: (空欄)</li>
					</ul>
				</object>
			</div>
		</div>
		<div class="l-tokyu-set__footer">
			<dl class="p-tokyu-set-data">
				<div>
					<dt>調査日</dt>
					<dd>2005年12月4日</dd>
				</div>
				<div>
					<dt>調査車両</dt>
					<dd>モ1806</dd>
				</div>
			</dl>
		</div>
	</div>

	<ul class="p-notes">
		<li>2006年2月25日、モ1806 にて変更がないことを確認。</li>
	</ul>
</Layout>
