---
import dayjs from 'dayjs';
import Layout from '@layouts/Tokyu.astro';
import Set from '@components/tokyu/Set.astro';
import SetPanelDial8500 from '@components/tokyu/SetPanelDial8500.astro';
import type { StructuredData } from '@type/types.js';

const structuredData: StructuredData = {
	title: '設定器配列　東急大井町線　8500系（2009年暫定）',
	heading: '大井町線　8500系',
	subHeading: '2009年暫定',
	dateModified: dayjs('2009-04-12'),
	breadcrumb: [
		{ path: '/tokyu/', name: '東急電車資料室' },
		{ path: '/tokyu/set/', name: '設定器配列' },
	],
	localNav: {
		label: '大井町線8500系の設定器バリエーション',
		items: [
			{ path: '/tokyu/set/8500_oimachi_2003', name: '2003年〜2009年' },
			{ path: '/tokyu/set/8500_oimachi_2009-05', name: '2009年暫定' },
			{ path: '/tokyu/set/8500_oimachi_2009-07', name: '2009年〜' },
		],
	},
};
---

<Layout astroFilePath={Astro.self.moduleId} structuredData={structuredData} toc={false}>
	<Set period="2009年7月の大井町線溝の口延伸に伴う暫定的なもの" note="下り方先頭車に設置" invDate="2009-04-12" invCar="デハ8540">
		<SetPanelDial8500 trainImage="8500_denentoshi_k_2003_train.png" toImage="8500_oimachi_2009-05_to.png">
			<ul slot="train">
				<li>1: 試運転</li>
				<li>2: 回送</li>
				<li>3: 普通</li>
				<li>4: 急行</li>
				<li>5: 快速</li>
				<li>6: 臨時</li>
				<li>7: (空欄)</li>
				<li>8: (空欄)</li>
				<li>9: (空欄)</li>
				<li>10: (空欄)</li>
			</ul>

			<ul slot="to">
				<li>1: (空欄)</li>
				<li>2: (空欄)</li>
