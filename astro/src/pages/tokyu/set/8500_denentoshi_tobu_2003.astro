import type { StructuredData } from '@type/types.js';

const structuredData: StructuredData = {
	title: '設定器配列　東急田園都市線　8500系（東武乗入れ編成、2003年〜2006年）',
	heading: '田園都市線　8500系',
	subHeading: '東武乗入れ編成、2003年〜2006年',
	dateModified: dayjs('2006-01-02'),
	breadcrumb: [
		{ path: '/tokyu/', name: '東急電車資料室' },
		{ path: '/tokyu/set/', name: '設定器配列' },
