---
import dayjs from 'dayjs';
import Layout from '@layouts/Tokyu.astro';
import type { StructuredData } from '@type/types.js';

const structuredData: StructuredData = {
	title: '設定器配列　東急池上、多摩川線　7600系、7700系、1000N系、1000N′系（2000年〜2018年）',
	heading: '池上、多摩川線　7600系、7700系、1000N系、1000N′系',
	subHeading: '2000年〜2018年',
	dateModified: dayjs('2016-03-06'),
	breadcrumb: [
		{ path: '/tokyu/', name: '東急電車資料室' },
		{ path: '/tokyu/set/', name: '設定器配列' },
	],
	localNav: [
		{ path: '/tokyu/set/7600_iketama_2000', name: '2000年〜2018年' },
		{ path: '/tokyu/set/1000_iketama_2014', name: '2014年〜（1000N系、1000N′系）' },
		{ path: '/tokyu/set/1500_iketama_2014', name: '2014年〜（1000系1500番台）' },
	],
};
---

<Layout astroFilePath={Astro.self.moduleId} structuredData={structuredData} toc={false}>
	<div class="l-tokyu-set">
		<div class="l-tokyu-set__header">
			<p class="p-tokyu-set-period">2000年<!-- 8月 -->の駅名変更から、2018年の7700系引退まで</p>
		</div>
		<div class="l-tokyu-set__main">
			<div class="p-tokyu-set-panel -push">
				<object data="/tokyu/set/image/7600_iketama_2000.svg" type="image/svg+xml" width="528" height="1170" class="p-tokyu-set-panel__image" role="img" aria-labelledby="set-image-alt1">
					<div id="set-image-alt1">
						<section>
							<h2>始発駅</h2>
							<table>
								<tbody>
									<tr>
										<td></td>
										<td></td>
										<td>渋谷</td>
										<td>田園調布</td>
										<td>元住吉</td>
										<td>日吉</td>
										<td>菊名</td>
										<td>桜木町</td>
									</tr>
									<tr>
										<td></td>
										<td></td>
										<td>北千住</td>
										<td>南千住</td>
										<td>八丁堀</td>
										<td></td>
										<td></td>
										<td>中目黒</td>
									</tr>
									<tr>
										<td>目黒</td>
										<td>大岡山</td>
										<td>奥沢</td>
										<td>多摩川</td>
										<td>蒲田</td>
										<td>雪が谷大塚</td>
										<td>五反田</td>
										<td></td>
									</tr>
								</tbody>
							</table>
						</section>

						<section>
							<h2>種別</h2>
							<table>
								<tbody>
									<tr>
										<td>普通</td>
										<td>急行</td>
										<td>快速</td>
										<td>日比谷線直通</td>
										<td>試運転</td>
										<td>回送</td>
										<td>臨時</td>
										<td></td>
									</tr>
								</tbody>
							</table>
						</section>

						<section>
							<h2>行先</h2>
							<table>
								<tbody>
									<tr>
										<td>渋谷</td>
										<td>田園調布</td>
										<td>元住吉</td>
										<td>日吉</td>
										<td>菊名</td>
										<td>桜木町</td>
										<td>北千住</td>
										<td>南千住</td>
									</tr>
									<tr>
										<td>八丁堀</td>
										<td>目黒</td>
										<td>大岡山</td>
										<td>奥沢</td>
										<td>多摩川</td>
										<td>蒲田</td>
										<td>雪が谷大塚</td>
										<td>五反田</td>
									</tr>
									<tr>
										<td>蒲田→多摩川</td>
										<td>蒲田→五反田</td>
										<td>蒲田→雪が谷大塚</td>
