									</tr>
									<tr>
										<td>区間準急</td>
										<td>通勤準急</td>
									</tr>
									<tr>
										<td>準急</td>
										<td>急行南停</td>
									</tr>
								</tbody>
							</table>
						</section>

						<section>
							<h2>行先</h2>
							<table>
								<tbody>
									<tr>
										<td>久喜</td>
										<td></td>
										<td></td>
										<td>中央林間</td>
										<td></td>
										<td>長津田</td>
