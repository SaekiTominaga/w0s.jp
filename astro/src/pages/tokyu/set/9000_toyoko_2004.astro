---
import dayjs from 'dayjs';
import Layout from '@layouts/Tokyu.astro';
import Set from '@components/tokyu/Set.astro';
import SetPanel from '@components/tokyu/SetPanel.astro';
import ListNote from '@components/ListNote.astro';
import type { StructuredData } from '@type/types.js';

const structuredData: StructuredData = {
	title: '設定器配列　東急東横線　9000系（2004年〜2013年）',
	heading: '東横線　9000系',
	subHeading: '2004年〜2013年',
	dateModified: dayjs('2006-01-01'),
	breadcrumb: [
		{ path: '/tokyu/', name: '東急電車資料室' },
		{ path: '/tokyu/set/', name: '設定器配列' },
	],
	localNav: {
		label: '東横線9000系の設定器バリエーション',
		items: [
			{ path: '/tokyu/set/9000_toyoko_2001', name: '2001年暫定' },
			{ path: '/tokyu/set/9000_toyoko_2004', name: '2004年〜2013年' },
		],
	},
};
---

<Layout astroFilePath={Astro.self.moduleId} structuredData={structuredData} toc={false}>
	<Set period="2004年1月の桜木町廃止から、2013年の東横線撤退まで" invDate="2006-01-01" invCar="クハ9013">
		<SetPanel image="9000_toyoko_2004.svg" imageWidth={528} imageHeight={1170}>
			<section>
				<h2>始発駅</h2>

				<table>
					<tbody>
						<tr>
							<td>渋谷</td>
							<td>元町・中華街</td>
							<td>元住吉</td>
							<td>日吉</td>
							<td>菊名</td>
							<td></td>
						</tr>
						<tr>
							<td>武蔵小杉</td>
							<td>横浜</td>
							<td></td>
							<td></td>
							<td></td>
							<td></td>
						</tr>
					</tbody>
				</table>
			</section>

			<section>
				<h2>種別</h2>

				<table>
					<tbody>
						<tr>
							<td>各停</td>
							<td>急行</td>
							<td>快速</td>
							<td>試運転</td>
							<td>回送</td>
							<td></td>
							<td>通勤特急</td>
							<td>特急</td>
						</tr>
					</tbody>
				</table>
			</section>

			<section>
				<h2>行先</h2>

				<table>
					<tbody>
						<tr>
							<td>渋谷</td>
							<td>元町・中華街</td>
							<td>元住吉</td>
							<td>日吉</td>
							<td>菊名</td>
							<td></td>
							<td></td>
							<td></td>
						</tr>
						<tr>
							<td></td>
							<td></td>
							<td></td>
							<td></td>
							<td></td>
							<td></td>
							<td></td>
							<td></td>
						</tr>
						<tr>
							<td>武蔵小杉</td>
							<td></td>
							<td>横浜</td>
							<td></td>
							<td></td>
							<td></td>
							<td></td>
							<td></td>
						</tr>
						<tr>
							<td></td>
