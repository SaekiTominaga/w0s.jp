---
import dayjs from 'dayjs';
import GithubSlugger from 'github-slugger';
import Layout from '@layouts/Tokyu.astro';
import Section from '@components/Section.astro';
import Image from '@components/Image.astro';
import type { StructuredData } from '@type/types.js';

const structuredData: StructuredData = {
	title: '東急電車の蓄電池',
	heading: '蓄電池',
	dateModified: dayjs('2005-11-30'),
	description: '7200系から新3000系までに搭載された蓄電池を紹介します。なお、このページは写真掲載のみで解説はありません。',
	image: 'https://media.w0s.jp/image/tokyu/sty/bt/8500_100V-40Ah.jpg',
	breadcrumb: [
		{ path: '/tokyu/', name: 'ホーム' },
		{ path: '/tokyu/sty/', name: '車両形態' },
	],
};

const slugger = new GithubSlugger();
---

<Layout astroFilePath={Astro.self.moduleId} structuredData={structuredData}>
	<Section slugger={slugger} id="series7200" heading="7200系">
		<div class="c-grid -wide -hdg-c">
			<div class="c-grid__item">
				<Section slugger={slugger} depth={2} heading="7200系" headingNote="100V・30Ah" headingType="c">
					<figure class="c-fit">
						<div class="p-embed -border">
							<a href="https://media.w0s.jp/image/tokyu/sty/bt/7200_100V-30Ah.jpg" type="image/jpeg">
								<Image path="tokyu/sty/bt/7200_100V-30Ah.jpg" alt="写真拡大" width={485} height={243} quality={60} />
								<!-- 撮影日: 2009-09-17 --></a
							>
						</div>
						<figcaption class="c-caption -meta">
							<span class="c-caption__title">デヤ7290（2次車）海側</span>
						</figcaption>
					</figure>
				</Section>
			</div>
		</div>
	</Section>

	<Section slugger={slugger} id="series8000" heading="8000系グループ、7600系、7700系">
		<div class="c-grid -wide -hdg-c">
			<div class="c-grid__item">
				<Section slugger={slugger} depth={2} heading="8000系" headingNote="100V・20Ah、100V・30Ah" headingType="c">
