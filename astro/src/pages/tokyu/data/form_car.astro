import type { StructuredData } from '@type/types.js';

const structuredData: StructuredData = {
	title: '東急電車　主要機器搭載表（車種別）',
	heading: '主要機器搭載表',
	subHeading: '車種別',
	dateModified: dayjs('2009-03-31'),
	description: '東急電鉄、横浜高速鉄道の車両（新5000系以降を除く）について、制御装置、補助電源装置、空気圧縮機、蓄電池の搭載状況を編成表形式で紹介します。',
	breadcrumb: [
		{ path: '/tokyu/', name: '東急電車資料室' },
		{ path: '/tokyu/data/', name: '車両データ' },
	],
	localNav: {
		label: '主要機器搭載表',
		items: [
			{ path: '/tokyu/data/form_car', name: '車種別' },
			{ path: '/tokyu/data/form_line', name: '路線別' },
		],
	},
};

const slugger = new GithubSlugger();
