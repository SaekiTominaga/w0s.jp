		<div class="p-table -scroll">
			<table>
				<thead>
					<tr>
						<th scope="col" rowspan="2">車号</th>
						<th scope="col" rowspan="2">発注</th>
						<th scope="col" colspan="3">購入</th>
						<th scope="col" colspan="3">フト10形化</th>
						<th scope="col" colspan="2">記号改正</th>
						<th scope="col">廃止</th>
					</tr>
					<tr>
						<th scope="col">申請</th>
						<th scope="col">認可</th>
						<th scope="col">竣功</th>
						<th scope="col">申請</th>
						<th scope="col">認可</th>
						<th scope="col">竣功</th>
						<th scope="col">実施</th>
						<th scope="col">報告</th>
						<th scope="col">届出</th>
					</tr>
				</thead>
