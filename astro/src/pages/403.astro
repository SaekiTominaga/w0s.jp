---
import Layout from '@layouts/Error.astro';
import type { StructuredDataErrorPage } from '@type/types.js';

const structuredData: StructuredDataErrorPage = {
	heading: 'このコンテンツにはアクセスできません',
	headingCode: '403 Forbidden',
	moduleScripts: ['error.mjs'],
};
---

<Layout structuredData={structuredData} search={true}>
	<p id="parentpage-msg" hidden="">「<a id="parentpage-anchor"></a>」でお探しのコンテンツが見つかるかもしれませんし、見つからないかもしれません。</p>
</Layout>
