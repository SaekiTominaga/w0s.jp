{
	"name": "frontend",
	"private": true,
	"type": "module",
	"scripts": {
		"predev": "rimraf -g \"../public/**/*.svg.br\" \"../public/assets/{script,style}/*.br\"",
		"dev": "pnpm run /^dev:.*/",
		"dev:css": "postcss style/*.css --base style -d ../public/assets/style -w",
		"dev:js": "rollup -c -w --environment build:development",
		"dev:svg": "node build/svg.ts -i image -o ../public -c svgo.config.js -w",
		"prebuild": "pnpm lint && rimraf -g \"../public/**/*.{svg,svg.br}\" \"../public/favicon.{ico,ico.br}\" \"../public/assets/{script,style}/*\"",
		"build": "pnpm run /^build:.*/",
		"build:css": "postcss style/*.css --base style -d ../public/assets/style",
		"build:js": "rollup -c --environment build:production",
		"build:svg": "node build/svg.ts -i image -o ../public -c svgo.config.js",
		"postbuild": "prettier --config ../.prettierrc.mjs -w \"../public/assets/script/*.{js,mjs}\" \"../public/assets/style/*.css\" && brotli-cli compress -g \"../public/assets/script/*.{js,mjs}\" \"../public/assets/style/*.css\" \"../public/**/*.svg\" \"../public/favicon.ico\" -m text",
		"lint": "pnpm run /^lint:.*/",
		"lint:css": "stylelint style/**/*.css",
		"lint:ts": "tsc --project script --noEmit",
		"lint:js": "eslint build/**/*.ts script/**/*.ts"
	},
	"dependencies": {
		"@w0s/base.css": "1.3.2",
		"@w0s/button-checkboxes": "3.0.3",
		"@w0s/button-clipboard": "3.0.4",
		"@w0s/button-confirm": "3.0.3",
		"@w0s/button-media-same-play": "3.0.4",
		"@w0s/closest-html-page": "5.0.1",
		"@w0s/details-animation": "7.0.4",
		"@w0s/footnote-reference-popover": "7.3.3",
		"@w0s/form-control-validation": "5.0.3",
		"@w0s/form-submit-overlay": "4.0.4",
		"@w0s/input-date-to-text": "4.0.5",
		"@w0s/input-switch": "6.0.0",
		"@w0s/report-js-error": "4.0.1",
		"@w0s/report-same-referrer": "5.0.0",
		"@w0s/string-convert": "3.0.3",
		"@w0s/table-cell-ditto": "4.1.1",
		"@w0s/textarea-auto-size": "4.0.3",
		"chokidar": "5.0.0",
		"slash": "5.1.0",
		"svgo": "4.0.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "29.0.0",
		"@rollup/plugin-node-resolve": "16.0.3",
		"@rollup/plugin-typescript": "12.3.0",
		"@types/trusted-types": "2.0.7",
		"postcss": "8.5.6",
		"postcss-cli": "11.0.1",
		"postcss-custom-media": "11.0.6",
		"postcss-discard-comments": "7.0.5",
		"postcss-discard-empty": "7.0.1",
		"postcss-import": "16.1.1",
		"postcss-nesting": "13.0.2",
		"rollup": "4.55.1"
	},
	"packageManager": "pnpm@10.28.1"
}
